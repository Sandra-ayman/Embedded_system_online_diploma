//homework 3 example 3 string
#include<stdio.h>
#include<string.h>
int main()
{
	char a[50],b[50];
	int i;
	printf("Enter the string : ");
	fflush(stdin);
	fflush(stdout);
	gets(a);
	for(i=strlen(a);i>0;i--)
	{
		b[i]=a[i];
	}
	printf("Reverse string is : %s",b);
}

