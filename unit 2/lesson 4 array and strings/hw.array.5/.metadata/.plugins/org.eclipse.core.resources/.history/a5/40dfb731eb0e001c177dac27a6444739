//homework example4 array
#include<stdio.h>
int main()
{
	int n,a[10],x,y,i=0;
	printf("Enter no of elements : ");
	fflush(stdin);
	fflush(stdout);
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		scanf("%d ",&a[i]);
	}
	printf("\n Enter the element to be inserted : ");
	fflush(stdin);
	fflush(stdout);
	scanf("%d",&x);
	printf("\n Enter the location : ");
	fflush(stdin);
	fflush(stdout);
	scanf("%d",&y);
	for(i=n;i>=y;i--)
	{
		a[i]=a[i-1];
	}
	n++;
	a[y-1]=x;
	for(i=0;i<n;i++)
	{
		printf("%d ",a[i]);
	}
	return 0;
}

