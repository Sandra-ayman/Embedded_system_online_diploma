//homework 4 example 3
#include<stdio.h>
void reversesentence(void)
{
	char a;
	scanf("%c",&a);
	if(a!='\0')
	{
		reversesentence();
		printf("%c",a);
	}
}
int main()
{
	printf("Enter a sentence :");
	reversesentence();
	return 0;
}

