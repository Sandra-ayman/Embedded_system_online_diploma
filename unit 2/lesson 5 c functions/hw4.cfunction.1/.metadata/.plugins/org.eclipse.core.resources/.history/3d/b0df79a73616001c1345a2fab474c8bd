//homework 4 example 1
#include<stdio.h>
int prime(int x)
{
	int y,i;
	for(i=x-1;i>0;i--)
	{
		if(x%i==0)
		{
			break;
		}
		else
		{
			y=x;
		}
	}
	return y;
}
int main()
{
	int x,y,i;
	printf("Enter two numbers(intervals): ");
	fflush(stdin);
	fflush(stdout);
	scanf("%d %d",&x,&y);
	printf("Prime numbers between %d and %d are:",x,y);
	for(i=x+1;i<y;i++)
	{
		printf("%d ",prime(i));
	}
	return 0;
}


