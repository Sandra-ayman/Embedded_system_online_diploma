#include "student.h"

int main()
{
	char choice[40];
	printf("Choose one of the following list:\n");
	printf("1-Add a student Record\n");
	printf("2-Delete a student Record\n");
	printf("Enter your choice: ");
	fflush(stdin);
	fflush(stdout);
	scanf("%s",choice);
	switch(atoi(choice))
	{
	case 1:
		Add_Student();
		break;
	case 2:
		Delete_Student();
		break;
	default:
		printf("wrong choice!");
		break;
	}
	return 0;
}

