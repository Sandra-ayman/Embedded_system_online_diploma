#ifndef STUDENT_H_
#define STUDENT_H_
//libraries
#include "stdio.h"
#include "conio.h"
#include "stdlib.h"
#include "string.h"

//effective data
struct SStudent_Data
{
	int ID;
	char name[40];
	float height;
};

//linked list for students
struct SStudent
{
	struct SStudent_Data student;
	struct SStudent* PNextStudent;
};
//pointer to the first student
struct SStudent* PFirstStudent=Null;

#endif /* STUDENT_H_ */
