//effective data
struct SStudent_Data
{
	int ID;
	char First_name[20];
//	char Last_name[20];
	float height;
};

//linked list for students
struct SStudent
{
	struct SStudent_Data student;
	struct SStudent* PNextStudent;
};

//Linked List APIs
//Add a New Student Record
